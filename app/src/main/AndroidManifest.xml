<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prabh.teamphi">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/team_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/team_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mvvm.activity.mainactivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mvvm.activity.taskactivity.TaskActivity"
            android:label="@string/title_activity_bill"
            android:theme="@style/AppTheme" />
        <activity android:name=".mvvm.activity.registeractivity.RegisterActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".mvvm.activity.AddItemActivity.AddItemActivity" />
        <activity android:name=".mvvm.activity.itemactivity.ItemActivity"></activity>
    </application>

</manifest>